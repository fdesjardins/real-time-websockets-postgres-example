{
  "name": "real-time-websockets-postgres-example",
  "version": "1.0.0",
  "description": "Postgres LISTEN/NOTIFY + JSON types + WebSockets to build an Elm-like real-time data API",
  "main": "index.js",
  "engines" : {
    "node" : ">= 7.7.0"
  },
  "scripts": {
    "start": "node examples/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "./node_modules/.bin/knex migrate:latest --knexfile examples/database/knexfile.js",
    "rollback": "./node_modules/.bin/knex migrate:rollback --knexfile examples/database/knexfile.js",
    "init": "npm run rollback; npm run migrate",
    "test": "eslint *.js && nyc ./node_modules/.bin/mocha --opts mocha.opts tests/test.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "knex": "^0.12.7",
    "koa": "^2.1.0",
    "koa-compress": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-router": "^5.4.0",
    "koa-static": "^3.0.0",
    "koa-websocket": "^3.0.1",
    "lodash.merge": "^4.6.0",
    "pg": "^6.1.4",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.2.0",
    "ws": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "koa-webpack": "^0.3.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "webpack": "^2.2.1"
  },
  "keywords": [
    "real-time",
    "postgresql",
    "websockets",
    "koa2",
    "redux",
    "react",
    "rxjs",
    "webpack",
    "babel",
    "boilerplate"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
